{
  "name": "ng-universal-demo",
  "version": "1.0.0",
  "main": "index.js",
  "repository": {},
  "scripts": {
    "start": "npm run build && npm run server",
    "build": "webpack",
    "build:aot": "webpack --env.aot --env.client & webpack --env.aot --env.server",
    "build:prod": "webpack --env.aot --env.client -p & webpack --env.aot --env.server",
    "build:prodna": "webpack --env.client -p & webpack --env.server",
    "prebuild": "npm run clean",
    "prebuild:aot": "npm run clean",
    "prebuild:prod": "npm run clean",
    "clean": "rimraf dist",
    "server": "nodemon dist/server.js",
    "watch": "webpack --watch",
    "copy": "cpx \"src/app/shared/images/**/*.*\" dist/img & cpx \"src/app/shared/audio/**/*.*\" dist/audio & cpx \"src/app/shared/videos/**/*.*\" dist/videos",
    "copymdist": "cpx \"dist/app.css\" ../backoffice-app/www & cpx \"dist/client.js\" ../backoffice-app/www",
    "copymsrc": "cpx \"src/app/shared/images/**/*.*\" ../backoffice-app/www/img & cpx \"src/app/shared/audio/**/*.*\" ../backoffice-app/www/audio & cpx \"src/app/shared/videos/**/*.*\" ../backoffice-app/www/videos",
    "copycss": "cpx \"main.1.style.css\" dist"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "^4.0.3",
    "@angular/common": "^4.0.3",
    "@angular/compiler": "^4.0.3",
    "@angular/compiler-cli": "^4.0.3",
    "@angular/core": "^4.0.3",
    "@angular/forms": "^4.0.3",
    "@angular/http": "^4.0.3",
    "@angular/platform-browser": "^4.0.3",
    "@angular/platform-browser-dynamic": "^4.0.3",
    "@angular/platform-server": "^4.0.3",
    "@angular/router": "^4.0.3",
    "@types/lodash": "^4.14.74",
    "@types/socket.io": "^1.4.29",
    "angular2-ladda": "^1.2.1",
    "bootstrap-sass": "^3.3.7",
    "chart.js": "^2.5.0",
    "express": "^4.15.2",
    "flatpickr": "^2.6.1",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "mydatepicker": "^2.0.6",
    "mydaterangepicker": "^4.1.2",
    "ng2-dnd": "^4.2.0",
    "ng2-toastr": "^4.1.0",
    "ng2-validation": "^4.2.0",
    "ngx-infinite-scroll": "^0.5.1",
    "pdfmake": "^0.1.31",
    "rxjs": "^5.2.0",
    "socket.io": "^2.0.3",
    "socket.io-client": "^2.0.3",
    "swiper": "^3.4.2",
    "ts-md5": "^1.2.2",
    "typescript": "^2.2.2",
    "xhr2": "^0.1.4",
    "zone.js": "^0.8.5"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^4.0.0",
    "@ngtools/webpack": "^1.2.14",
    "@types/express": "^4.0.35",
    "@types/node": "^7.0.8",
    "cpx": "^1.5.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "image-webpack-loader": "^3.3.0",
    "node-sass": "^4.5.2",
    "nodemon": "^1.11.0",
    "optimize-css-assets-webpack-plugin": "^1.3.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "script-ext-html-webpack-plugin": "^1.7.1",
    "typescript": "^2.2.1",
    "webpack": "^2.2.1",
    "webpack-merge": "^4.0.0"
  }
